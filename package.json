{
  "name": "cosmos",
  "version": "0.2.3",
  "engine": "^7.0.0",
  "private": true,
  "workspaces": [
    "src/components",
    "src/tokens",
    "src/codemods",
    "src/overview",
    "src/babel-preset",
    "internal/docs",
    "examples/manage",
    "examples/webpack-hello-world"
  ],
  "dependencies": {
    "fuzzysearch": "1.0.3",
