{
  "name": "cosmos",
  "version": "0.9.0",
  "engine": "^7.0.0",
  "private": true,
  "workspaces": [
    "core/components",
    "core/tokens",
    "core/babel-preset",
    "core/codemods",
    "internal/cosmos-scripts",
    "internal/overview",
    "internal/docs",
    "internal/test",
    "examples/manage",
    "examples/webpack-hello-world",
    "examples/perf-tests"
  ],
  "scripts": {
    "scripts": "nps -s",
    "i": "nps-i",
    "dev": "echo Our scripts have changed, run yarn scripts to know more",
    "build": "nps -s production.build",
    "start": "nps -s production.start",
    "test": "nps -s test",
    "sandbox": "start-storybook -p 9001 -s build",
    "precommit": "nps icons.build && lint-staged",
    "chromatic": "chromatic test --storybook-addon --script-name sandbox --exit-zero-on-changes"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 100
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-actions": "4.0.8",
    "@storybook/addon-links": "4.0.8",
    "@storybook/addons": "4.0.8",
    "@storybook/react": "4.0.8",
    "babel-loader": "7.1.4",
    "babel-cli": "6.26.0",
    "babel-jest": "^23.4.2",
    "chokidar": "2.0.4",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "execa": "0.10.0",
    "fs-extra": "6.0.1",
    "htmlparser2": "3.9.2",
    "jest": "^23.5.0",
    "latest-version": "4.0.0",
    "lint-staged": "7.2.0",
    "lodash.camelcase": "4.3.0",
    "lodash.frompairs": "4.0.1",
    "nps": "5.9.2",
    "nps-i": "1.0.2",
    "nps-utils": "1.6.0",
    "prettier": "1.14.3",
    "prettycli": "1.4.3",
    "react-chromatic": "0.8.4",
    "react-docgen": "2.21.0",
    "react-docgen-deprecation-handler": "1.0.0",
    "react-docgen-displayname-handler": "2.1.0",
    "read-pkg": "4.0.1",
    "svgo": "1.0.5"
  }
}
